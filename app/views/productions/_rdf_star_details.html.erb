<h2 title='<%= prop %>'>
    <%= display_label(prop) %>    
</h2>

<% value.each do |statement| %>
  <% if statement["@id"] %>
    <a href='<%= statement["@id"] %>'>
      <%= display_label(statement["@id"]) %> 
    </a> 
  <% else %>
   <a> <%= display_literal(statement) %></a>
  <% end %>

  <% if statement["@annotation"] %>
    <%= render partial: "production_annotation" , locals: {annotations: statement["@annotation"] }%>
  <% end %>


<% end %>
